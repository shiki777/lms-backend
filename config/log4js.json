{
  "appenders": [{
    "type": "clustered",
    "appenders": [{
      "type": "dateFile",
      "filename": "log/access.log",
      "pattern": "-yyyy-MM-dd",
      "category": "http"
    }, {
      "type": "file",
      "filename": "log/channel.log",
      "category": "channel"
    },{
      "type": "file",
      "filename": "log/program.log",
      "category": "program"
    },{
      "type": "file",
      "filename": "log/callback.log",
      "category": "callback"
    }, {
      "type": "file",
      "filename": "log/upload.log",
      "category": "upload"
    }, {
      "type": "file",
      "filename": "log/user.log",
      "category": "user"
    }, {
      "type": "logLevelFilter",
      "level": "ERROR",
      "category": "error",
      "appender": {
        "type": "file",
        "filename": "log/errors.log"
      }
    }]
  }]
}